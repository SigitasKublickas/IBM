// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Car {
  id Int @id @default(autoincrement())
  plate String
  time_stamp DateTime
  
}

model PriceChart {
 id Int @id @default(autoincrement())
 price Float
 time DateTime
} 
